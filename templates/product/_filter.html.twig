{{ form_start(form, {attr: {class: 'filter'}}) }}

<h4>Shop Name :</h4>
{{ form_row(form.shop) }}
<h4>Price:</h4>
<div class="row">
    <div class="col-md-6">
        {{form_row(form.min)}}
    </div>
    <div class="col-md-6">
        {{form_row(form.max)}}
    </div>
</div>
<h4>Stock:</h4>
{{form_row(form.stock)}}

<button type="submit" class="btn btn-primary w-10">Search</button>



{{  form_end(form) }}